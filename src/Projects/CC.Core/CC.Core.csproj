    <PackageReference Include="Microsoft.ReportViewer.Web.2012" Version="1.0.0" />
    <PackageReference Include="NHibernate" Version="3.4.1.4000" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="5.0.0" />
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
    <PackageReference Include="Microsoft.DotNet.UpgradeAssistant.Extensions.Default.Analyzers" Version="0.2.233001">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="structuremap" Version="4.7.1" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Core\Html\Expressions\AlphaNumericPickerExpression.cs" />
