    <Compile Remove="Core\Html\Expressions\SubmitButtonExpression.cs" />
    <Compile Remove="Core\Html\Expressions\ValueObjectSelector.cs" />
    <Compile Remove="DataValidation\Attributes\EqualTo.cs" />
