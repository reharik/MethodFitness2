@model CalendarViewModel
@using MethodFitness.Core.CoreViewModelAndDTOs
@using HtmlTags
@using MethodFitness.Core.Html.FubuUI
<script>
    var calendarDef=  @MvcHtmlString.Create(JsonUtil.ToJson(Model.CalendarDefinition));
    calendarDef.calendarContainer="#calendar";
    $(document).ready(function(){
        new mf.AppointmentCalendarController(calendarDef,{height:"800px"});
        });

</script>

<div class="content-header">
    @Html.SubmissionFor(x=>x.Location).HideLabel().ToHtmlTag()
    <span class="title-name">@Model.Title</span>
</div>
<div id="contentArea" class="content-inner">
    <div id="errorMessagesCalendar" class="dci_ErrorMessages errorMessages"><ul></ul></div>
        <div id='calendar'></div>
        <div class="footer">
    </div>
</div>


