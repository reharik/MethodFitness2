@using CC.Core.Html.CCUI
@using MethodFitness.Web
@model MethodFitness.Web.Areas.Reports.Controllers.DailyPaymentViewModel

@{
    Layout = "~/Views/Shared/_ReportLayout.cshtml";
}
<div class="col full">
    <div class="fieldRow">
        @Html.SubmissionFor(x=>x.Date).AddClassToRoot("one-third").ToHtmlTag()
    </div>
    <div class="fieldRow">
        @StandardButtonFor("viewReport", WebLocalizationKeys.VIEW_REPORT).AddClass("save").ToHtmlTag()
    </div>
</div>

<div class="col full">
    <div class="fieldRow">
        <iframe width="900px" height="900px" id="reportBody"></iframe>
    </div>
</div>

