body {
	background-color: #F2F2F2;
	margin: 0;
	font-family: Verdana;
	font-size: 14px;
}

#main-header {
	background: url(../images/header-title.png) no-repeat scroll top left
		#407397;
	height: 71px;
	min-width: 870px;
	display: block;
	position: relative;
}

/*******   HEADER TABS ********/
#main-header ul#main-tabs {
	bottom: 1px;
	display: block;
	height: 32px;
	margin: 0 0 0 160px;
	position: absolute;
}

#main-header ul#right-tabs {
	bottom: 1px;
	right: 0;
	display: block;
	height: 32px;
	margin: 0 20px 0 0;
	position: absolute;
}

#main-header ul li {
	background: url(../images/maintab-ns.png) repeat-x scroll 0 0 #94bed1;
	border-top: 1px solid #C0D4DE;
	display: block;
	float: left;
	list-style: none outside none;
	margin: 0 0 0 3px;
	padding: 0 20px;
	height: 32px;
	-webkit-border-top-left-radius: 3px;
	-webkit-border-top-right-radius: 3px;
	-moz-border-radius-topleft: 3px;
	-moz-border-radius-topright: 3px;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
}

#main-header ul li a {
	display: block;
	line-height: 32px;
	text-align: center;
	width: 100%;
	text-decoration: none;
	color: white;
	font-size: 0.9em;
	text-shadow: -1px -1px 1px #757575;
	filter: dropshadow(color =   #757575, offx =   -1, offy =   -1);
}

#portfolios #main-header ul#main-tabs li#portfolios-tab {
	background: white;
	border-top: medium none;
	height: 33px;
}

#portfolios #main-header ul#main-tabs li#portfolios-tab a {
	color: #333333;
	text-shadow: -1px -1px 1px #DDDDDD;
	filter: dropshadow(color =   #DDDDDD, offx =   -1, offy =   -1);
}

#main-header ul li#chart-tab,#main-header ul li#group-tab {
	padding: 0 10px;
}

#main-header ul li#chart-tab a {
	background: url(../images/chart-tab-icon-ns.png) no-repeat scroll 0 6px
		transparent;
	width: 22px;
}

#main-header ul li#group-tab a {
	background: url(../images/group-tab-icon-ns.png) no-repeat scroll 0 6px
		transparent;
	width: 22px;
}

#chart #main-header ul li#chart-tab {
	background: white;
}

#chart #main-header ul li#chart-tab a {
	background: url(../images/chart-tab-icon-s.png) no-repeat scroll 0 6px
		transparent;
}

#group #main-header ul li#group-tab {
	background: white;
}

#group ul li#group-tab a {
	background: url(../images/group-tab-icon-s.png) no-repeat scroll 0 6px
		transparent;
}

#login-links {
	position: absolute;
	right: 16px;
	top: 5px;
}

#login-links  a {
	border-left: 1px solid #F6F6F6;
	color: white;
	font-family: verdana;
	font-size: 10px;
	font-weight: bold;
	padding: 0 3px 0 5px;
	text-decoration: none;
}

#login-links  a.first {
	border-left: none;
}

#main-body {
	background: url(../images/main-content.png) repeat-x scroll top left
		#f2f2f2;
	border-top: 1px solid #e5e5e5;
	margin: 0;
	min-height: 500px;
}

#top-separator {
	height: 7px;
	display: block;
	background: white;
}

.clear {
	clear: both;
}

/******  LEFT NAVIGATION  *******/
#left-navigation {
	float: left;
	width: 200px;
}

#title-location {
	background: url(../images/left-nav-title.png) repeat-x scroll 0 0
		#71a8c2;
	-webkit-border--radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	border-top: 1px solid #cadee8;
	margin: 10px 0 0 5px;
	padding: 4px 2px 4px 6px;
	width: 177px;
}

#title-location #bread-crumb {
	color: #457495;
	font-size: 0.67em;
	font-weight: bold;
}

#title-location #bread-crumb a {
	color: #457495;
}

#title-location h3 {
	color: #FFFFFF;
	font-size: 0.9em;
	margin: 5px 0;
}

#left-navigation ul {
	padding: 6px;
}

#left-navigation ul li {
	list-style: none;
	height: 25px;
}

#left-navigation ul li a {
	text-decoration: none;
	color: 457596;
	font-size: 0.8em;
	padding: 0 0 0 8px;
}

#left-navigation ul li.active {
	background: url(../images/left-nav-select.png) no-repeat scroll 0 0
		transparent;
}

#left-navigation ul li.active a {
	font-weight: bold;
}

#left-navigation ul li.haschild {
	background: url(../images/left-nav-haschild.png) no-repeat scroll right
		0 transparent;
}

#left-navigation ul li.not-available {
	color: #bfbfbf;
	font-size: 0.8em;
	padding: 0 0 0 8px;
}

#left-navigation ul li.not-available a {
	color: #447292;
	float: right;
	width: 50px;
	font-size: 0.9em;
	display: block;
}

/******  MAIN CONTENT WRAPPERS  *******/
#main-content {
	margin: 10px 16px 0 200px;
}

.content-outer {
	width: 100%;
}

.content-outer .content-inner {
	min-height: 400px;
	background: white;
	padding-bottom: 3px;
}

.content-outer .content-footer {
	height: 17px;
	width: 100%;
	background: url(../images/footer-shadow-left.png) no-repeat scroll 0 0
		transparent;
}

.content-outer .content-footer span {
	background: url(../images/footer-shadow-right.png) no-repeat scroll 0 0
		transparent;
	width: 307px;
	height: 17px;
	float: right;
}

/******  MAIN CONTENT HEADER  *******/
.content-outer .content-header {
	width: 100%;
	height: 41px;
	background: url(../images/content-header-silver.png) repeat-x scroll 0 0
		transparent;
	position: relative;
}

.blue .content-header {
	background: url(../images/content-header-blue.png) repeat-x scroll 0 0
		transparent;
	border-bottom: medium none;
}

.content-outer .content-header .search {
	background: url(../images/search-bg.png) no-repeat scroll 0 0
		transparent;
	width: 178px;
	height: 25px;
	position: absolute;
	right: 20px;
	top: 8px;
	border: none;
	padding: 0 10px 0 30px;
	font-size: 0.8em;
}

.content-outer .content-header .close {
	background: url(../images/close-x.png) no-repeat scroll 0 0 transparent;
	width: 14px;
	height: 16px;
	position: absolute;
	right: 20px;
	top: 5px;
	border: none;
	cursor: pointer;
}

.content-outer .content-header select {
	float: left;
	height: 23px;
	width: 140px;
	margin: 8px 10px 0 6px;
}

.content-outer .content-header button {
	filter: progid :   DXImageTransform.Microsoft.gradient (
		startColorstr =
		 '#f9f9f9', endColorstr =   '#e9e9e9', GradientType =   0 );
	/* IE6-9 */
	background: linear-gradient(top, #f9f9f9 0%, #e9e9e9 100%); /* W3C */
	font-weight: bold;
	display: block;
	width: 29px;
	height: 23px;
	padding: 0;
	border: 1px solid #c6c6c6;
	float: left;
	-webkit-border--radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	margin: 8px 0 0 6px;
}

.content-outer .content-header button.delete {
	background: url(../images/icon-header-delete.png) no-repeat scroll 0 0
		transparent;
}

.content-outer .content-header button.addToProfile {
	background: url(../images/icon-header-new.png) no-repeat scroll 0 0
		transparent;
	width: 38px;
	margin-right: 10px;
}

.content-outer .content-header button.new {
	background: url(../images/icon-header-plus.png) no-repeat scroll 0 0
		transparent;
}

.content-outer .content-header button.print {
	background: url(../images/icon-header-print.png) no-repeat scroll 0 0
		transparent;
}

.content-outer .content-header button.copy {
	background: url(../images/icon-header-pages.png) no-repeat scroll 0 0
		transparent;
}

.content-outer .content-header button.arrow {
	background: url(../images/icon-header-arrow.png) no-repeat scroll 0 0
		transparent;
}

.content-outer .content-header span {
	display: block;
	text-align: center;
	width: 90%;
}

.content-outer .content-header .title-name {
	padding: 5px 0 0;
	color: #666666;
	font-size: 1em;
}

.content-outer .content-header .title-added {
	color: #999999;
	font-size: 0.8em;
}

/******  FORMS  *******/
form div.col {
	width: 335px;
	padding: 10px 20px;
	display: block;
}

form div.footer {
	width: 100%;
	display: block;
	clear: both;
	padding: 10px 0 10px 30px;
}

form div.left {
	float: left;
}

form div.right {
	float: left;
}

form .float-right {
	float:right
}

form div.fieldRow {
	font-size: 11px;
	color: #666666;
	display: block;
	margin: 8px 10px;
	overflow: hidden;
}

form div.fieldRow label {
	width: 100%;
	margin-bottom: 4px;
	display: block;
}

form div.fieldRow a {
	color: #666666;
	text-decoration: underline;
}

form div.fieldRow label.checkbox {
	float: left;
	line-height: 25px;
	margin-left: 7px;
	margin-top: 2px;
	width: 60%;
}

form div.fieldRow div.one-quarter {
	margin-left: 2%;
	width: 23%;
	float: left;
}

form div.fieldRow div.one-third {
	margin-left: 2%;
	width: 31%;
	float: left;
}

form div.fieldRow div.half {
	margin-left: 2%;
	width: 48%;
	float: left;
}

form div.fieldRow div.two-thirds {
	margin-left: 2%;
	width: 64%;
	float: left;
}

form div.fieldRow div.three-quarters {
	margin-left: 2%;
	width: 73%;
	float: left;
}

form div.fieldRow div.first {
	margin-left: 0;
	margin-right: 2%;
}

form div.fieldRow input,form div.fieldRow select {
	width: 100%;
	height: 25px;
	color: #838383;
}

form div.fieldRow input.checkbox {
	float: left;
	width: 12px;
}

form div.fieldRow input.num,form div.fieldRow input.money {
	width: 40%;
}

form div.horizontal-label label {
	float: left;
	margin-top: 20px;
	width: 24%;
}

form div.horizontal-label input,form div.horizontal-label select {
	margin-top: 16px;
	width: 75%;
}

.footer button {
	-webkit-border--radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	font-size: 0.8em;
	font-weight: bold;
}

.footer button.save {
	background: url("../images/button-save-bg.png") repeat-x scroll 0 0
		#CCCCCC;
	border: 1px solid #B7D6DE;
	height: 27px;
	width: 80px;
	color: #437182;
}

.footer button.cancel {
	background: #dfdfdf;
	border: 1px solid #dfdfdf;
	height: 27px;
	width: 80px;
	color: #8F8F8F;
}

.ui-jqgrid .ui-jqgrid-htable th div {
	text-align: left;
	padding-left: 10px;
}

.ui-jqgrid-htable th  #jqgh_cb {
	padding-left: 0;
}

/******  JQGRID EXTENSION  *******/
#gbox_gridContainer {
	border-bottom: none;
}

#gview_gridContainer .ui-jqgrid-bdiv {
	overflow-x: hidden;
	overflow-y: auto;
}

.ui-jqgrid tr.ui-row-ltr td {
	font-family: verdana;
	border-right: none;
	border-left: none;
	border-bottom: 1px solid #e4e4e4;
	color: #666666;
	height: 28px;
	font-size: 0.7em;
}

.ui-jqgrid tr.ui-row-ltr td a {
	color: #447292;
	text-decoration: none;
}

.ui-jqgrid .ui-jqgrid-htable th {
	height: 28px;
}

.ui-th-ltr,.ui-jqgrid .ui-jqgrid-htable th {
	font-size: 0.7em;
}

.ui-th-ltr,.ui-jqgrid .ui-jqgrid-htable th.ui-th-ltr {
	background: url(../images/header-silver.png) repeat-x scroll 0 0 #eeeeee;
    font-weight:bold;

}

.ui-jqgrid .ui-jqgrid-htable th.is-sorted {
	background: #e2e2e2;
	-webkit-box-shadow: inset 3px 3px 12px 1px rgba(204, 204, 204, 1);
	-moz-box-shadow: inset 3px 3px 12px 1px rgba(204, 204, 204, 1);
	box-shadow: inset 3px 3px 12px 1px rgba(204, 204, 204, 1);
}

.blue .ui-jqgrid-htable th.ui-th-ltr {
	background: url(../images/header-blue.png) repeat-x scroll 0 0 #eeeeee;
}

.ui-jqgrid .ui-jqgrid-htable th.is-sorted {
	background: #e2e2e2;
	-webkit-box-shadow: inset 3px 3px 12px 1px rgba(204, 204, 204, 1);
	-moz-box-shadow: inset 3px 3px 12px 1px rgba(204, 204, 204, 1);
	box-shadow: inset 3px 3px 12px 1px rgba(204, 204, 204, 1);
}

.ui-jqgrid tr.ui-priority-secondary {
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
	filter: alpha(opacity =   100);
	-moz-opacity: 1;
	opacity: 1;
}

.ui-jqgrid tr.ui-priority-secondary td {
	background: #f6f6f6;
	color: #666666;
}

.ui-jqgrid tr.ui-state-hover td {
	background: #fae8b3; /* Old browsers */
	background: -moz-linear-gradient(top, #fae8b3 0%, #f9e1a0 100%);
	/* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #fae8b3),
		color-stop(100%, #f9e1a0) ); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #fae8b3 0%, #f9e1a0 100%);
	/* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #fae8b3 0%, #f9e1a0 100%);
	/* Opera11.10+ */
	background: -ms-linear-gradient(top, #fae8b3 0%, #f9e1a0 100%);
	/* IE10+ */
	filter: progid :   DXImageTransform.Microsoft.gradient (
		startColorstr =
		 '#fae8b3', endColorstr =   '#f9e1a0', GradientType =   0 );
	/* IE6-9 */
	background: linear-gradient(top, #fae8b3 0%, #f9e1a0 100%); /* W3C */
}

.ui-jqgrid tr.ui-state-highlight td {
	background: #fae8b3; /* Old browsers */
	background: -moz-linear-gradient(top, #fae8b3 0%, #f9e1a0 100%);
	/* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #fae8b3),
		color-stop(100%, #f9e1a0) ); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #fae8b3 0%, #f9e1a0 100%);
	/* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #fae8b3 0%, #f9e1a0 100%);
	/* Opera11.10+ */
	background: -ms-linear-gradient(top, #fae8b3 0%, #f9e1a0 100%);
	/* IE10+ */
	filter: progid :   DXImageTransform.Microsoft.gradient (
		startColorstr =
		 '#fae8b3', endColorstr =   '#f9e1a0', GradientType =   0 );
	/* IE6-9 */
	background: linear-gradient(top, #fae8b3 0%, #f9e1a0 100%); /* W3C */
}

.ui-jqgrid tr.jqgroup td {
	background: #fae8b3; /* Old browsers */
	background: -moz-linear-gradient(top, #e1e1e1 0%, #d7d7d7 100%);
	/* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #e1e1e1),
		color-stop(100%, #d7d7d7) ); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #e1e1e1 0%, #d7d7d7 100%);
	/* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #e1e1e1 0%, #d7d7d7 100%);
	/* Opera11.10+ */
	background: -ms-linear-gradient(top, #e1e1e1 0%, #d7d7d7 100%);
	/* IE10+ */
	filter: progid :   DXImageTransform.Microsoft.gradient (
		startColorstr =
		 '#e1e1e1', endColorstr =   '#d7d7d7', GradientType =   0 );
	/* IE6-9 */
	background: linear-gradient(top, #e1e1e1 0%, #d7d7d7 100%); /* W3C */
	font-weight: bold;
}

.ui-widget-content {
	border: 1px solid #d9d9d9;
}

/******  UTILS  *******/
.some-top-padding {
	padding-top: 20px !important;
}

/******   DIALOGS  **********/
.ui-dialog iframe {
	overflow-x: hidden;
}

.ui-dialog .ui-dialog-content {
	padding: 0;
}

/***********DEVINDA START***********************/
.full-width {
	width: 100%;
}

#main-content .radio {
	width: 10px;
	float: left;
}

/***********DEVINDA END***********************/
/***********priyanke starts***********************/
hr {
	width: 690px;
	float: left;
	margin: 0 0 0 30px
}

form div.footer div.col {
	padding: 10px 20px 10px 0;
}

#main-content .date-option {
	width: 40%;
	float: left;
	margin: 0 10px 0 0;
}

.content-outer .content-inner h3 {
	font-size: 12px;
	color: #666666;
	font-weight: bold;
	margin: 10px 0 10px 30px;
	width: 690px;
}

.bottom_cont {
	margin: 10px 0;
	float: left;
}

.bottom_cont ul {
	margin: 10px 0 10px 30px;
	padding: 0;
	width: 690px;
	float: left;
}

.bottom_cont ul li {
	list-style: none;
	float: left;
	width: 690px;
	margin: 0 0 5px 0;
}

.bottom_cont ul li span {
	color: #666666;
	font-size: 0.7em;
}

.bottom_cont ul li span input[type="text"] {
	width: 100%;
}

.bottom_cont ul li span.long_span {
	width: 19%;
	float: left;
	margin: 0 1% 0 0;
}

.bottom_cont ul li span.small_span {
	width: 9%;
	float: left;
	margin: 0 1% 0 0;
}

.close {
	background: url("../images/close-x.png") no-repeat scroll 0 0
		transparent;
	border: none;
	cursor: pointer;
	height: 16px;
	right: 20px;
	top: 5px;
	width: 14px;
}

/***********priyanke ends***********************/
/***********LAHIRU START***********************/
ul.plain-list {
	list-style-type: none;
	padding-left: 8px;
}

ul.plain-list a {
	text-decoration: none;
}
/***********LAHIRU END***********************/