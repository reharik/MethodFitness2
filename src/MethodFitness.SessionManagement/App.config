    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" requirePermission="false" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <add key="SystemSupport.SiteConfiguration" value="{
         Name             : 'System Support',
         Host             : 'http://localhost:666',
         LanguageDefault  : 'en-US',
         ScriptsPath      : '~/content/scripts/',
         CssPath          : '~/content/css/',
         ImagesPath       : '/content/images/',
         WebSiteRoot      : 'SystemSupport.Web',
         TrainerClientRateDefault :'65',
         jsApplicationName:'MF'
         }" />
  
  </appSettings>
  
  <log4net>
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="F:\\websites\\logs\\SessionManagement.log" />
      <appendToFile value="true" />
      <datePattern value="yyyy.MM.dd" />
      <maxSizeRollBackups value="-1" />
      <maximumFileSize value="20MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%timestamp %date (%property{log4net:HostName})[%thread] %-5level %logger [%method(%line)] - %message %exception%newline" />
      </layout>
    </appender>
    <logger name="NHibernate" additivity="false">
      <level value="ERROR"/>
      <appender-ref ref="NHibernateFileLog"/>
    </logger>
    <logger name="NHibernate.SQL" additivity="false">
      <level value="ERROR"/>
      <appender-ref ref="NHibernateFileLog"/>
    </logger>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="RollingLogFileAppender" />
    </root>
  </log4net>
  
